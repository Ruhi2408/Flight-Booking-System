package com.app.FlightSystem.repository;

import java.time.LocalDate;
import java.util.List;

import org.springframework.data.jpa.repository.JpaRepository;

import com.app.FlightSystem.model.Flight;

public interface FlightRepository extends JpaRepository<Flight,String> {
	List<Flight> findBySource_locationAndDestinationAndDepartureDate(String source_location,String destination,LocalDate departureDate);

}
